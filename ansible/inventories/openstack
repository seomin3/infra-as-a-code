[scef]
scef-openstack-controller01 ansible_ssh_host=192.168.5.43
scef-openstack-compute01 ansible_ssh_host=192.168.5.44
scef-openstack-compute02 ansible_ssh_host=192.168.5.45

[scef:vars]
ansible_user=root
ansible_ssh_private_key_file=/root/.ssh/id_rsa
ansible_ssh_common_args='-o StrictHostKeyChecking=no'

